{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "@components": "./components",
        "@styles": "./styles",
        "@utils": "./utils",
        "@workers": "./src/workers",
        "@typings": "./typings",
        "@assets":  "./assets",
        "@constants": "./constants",
        "@hooks": "./src/hooks"
      }
    }]
  ]
}
